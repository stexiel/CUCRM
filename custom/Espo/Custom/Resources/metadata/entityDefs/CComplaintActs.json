{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "account": {
            "type": "link"
        },
        "count": {
            "type": "int",
            "isCustom": true
        },
        "date": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "file": {
            "type": "file",
            "sourceList": [],
            "accept": [
                ".pdf",
                ".odt",
                ".ods",
                ".odp",
                ".docx",
                ".xlsx",
                ".pptx",
                ".doc",
                ".xls",
                ".ppt",
                ".rtf",
                ".csv",
                ".md",
                ".txt"
            ],
            "isCustom": true
        },
        "nameStore": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "nameWarehouse": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "storeCode": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "summ": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "warehoseCode": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "documentFolder": {
            "type": "link"
        },
        "instruction": {
            "type": "link"
        },
        "storeStatus": {
            "type": "link"
        },
        "warehouse": {
            "type": "link"
        },
        "sInstruction": {
            "type": "link"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "cComplaintActses",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "file": {
            "type": "belongsTo",
            "entity": "Attachment",
            "skipOrmDefs": true,
            "utility": true
        },
        "documentFolder": {
            "type": "belongsTo",
            "foreign": "cComplaintActses",
            "entity": "DocumentFolder",
            "audited": false,
            "isCustom": true
        },
        "instruction": {
            "type": "belongsTo",
            "foreign": "complaintActs",
            "entity": "CInstruction",
            "isCustom": true
        },
        "storeStatus": {
            "type": "belongsTo",
            "foreign": "complaintActses",
            "entity": "CStoreStatus",
            "audited": false,
            "isCustom": true
        },
        "warehouse": {
            "type": "belongsTo",
            "foreign": "complaintActses",
            "entity": "CWarehouse",
            "audited": false,
            "isCustom": true
        },
        "sInstruction": {
            "type": "belongsTo",
            "foreign": "complaintActs",
            "entity": "CSInstruction",
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}