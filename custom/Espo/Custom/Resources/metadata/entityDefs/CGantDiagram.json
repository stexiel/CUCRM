{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "startDate": {
            "notNull": false,
            "type": "date",
            "required": true,
            "isCustom": true,
            "default": "javascript: return this.dateTime.getToday();"
        },
        "preProjectJobs": {
            "notNull": true,
            "type": "bool",
            "default": true,
            "isCustom": true
        },
        "permissions": {
            "notNull": true,
            "type": "bool",
            "default": true,
            "isCustom": true
        },
        "operations": {
            "notNull": true,
            "type": "bool",
            "default": true,
            "isCustom": true
        },
        "marketing": {
            "notNull": true,
            "type": "bool",
            "default": true,
            "isCustom": true
        },
        "locationFind": {
            "notNull": false,
            "type": "date",
            "required": true,
            "default": "javascript: return this.dateTime.getToday();",
            "isCustom": true
        },
        "locationFindFinish": {
            "notNull": false,
            "type": "date",
            "isCustom": true,
            "readOnly": true
        },
        "locationFindDays": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true,
            "default": "5",
            "readOnly": true
        },
        "locationFindLabel": {
            "type": "text",
            "default": "Поиск локации и анализ недочётов",
            "rowsMin": 2,
            "cutHeight": 200,
            "readOnly": true,
            "isCustom": true
        },
        "secondLabel": {
            "type": "text",
            "default": "Сантехника (трубы в подвале,инженерные коммуникации, люки, соседи)",
            "rowsMin": 2,
            "cutHeight": 200,
            "readOnly": true,
            "isCustom": true
        },
        "secondDays": {
            "type": "varchar",
            "default": "1",
            "maxLength": 100,
            "options": [],
            "readOnly": true,
            "isCustom": true
        },
        "secondStartDate": {
            "notNull": false,
            "type": "date",
            "default": "javascript: return this.dateTime.getToday();",
            "isCustom": true
        },
        "secondFinishDate": {
            "notNull": false,
            "type": "date",
            "readOnly": true,
            "isCustom": true
        },
        "start3": {
            "notNull": false,
            "type": "date",
            "isCustom": true,
            "default": "javascript: return this.dateTime.getToday();"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}