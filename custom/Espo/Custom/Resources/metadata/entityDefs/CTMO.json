{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "storeStatus": {
            "type": "link"
        },
        "amountOfGoodsReceived": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "collection": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "consignmentNote": {
            "type": "file",
            "sourceList": [],
            "accept": [],
            "isCustom": true
        },
        "dateOfReceiptOfGoods": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "pOSHalyk": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "pOSKaspi": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "quantity": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "returnAmountOfGoods": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "tTTotalRevenue": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "uID": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "writeoff": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "supplier": {
            "type": "link"
        },
        "ptncd": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "ptnnm": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "storeStatus": {
            "type": "belongsTo",
            "foreign": "tMOs",
            "entity": "CStoreStatus",
            "audited": false,
            "isCustom": true
        },
        "consignmentNote": {
            "type": "belongsTo",
            "entity": "Attachment",
            "skipOrmDefs": true,
            "utility": true
        },
        "supplier": {
            "type": "belongsTo",
            "foreign": "tMOs",
            "entity": "CSupplier",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "dateOfReceiptOfGoods",
        "order": "desc",
        "textFilterFields": [
            "name",
            "storeStatus.name",
            "storeStatus.storeCode"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}