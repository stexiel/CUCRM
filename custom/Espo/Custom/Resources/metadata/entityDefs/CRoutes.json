{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "account": {
            "type": "link"
        },
        "employeeses": {
            "type": "linkMultiple"
        },
        "addressDivision": {
            "type": "address",
            "isCustom": true
        },
        "arrivalTime": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "autoNumbMark": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "estimatedTimeOfArrival": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "time": {
            "type": "enum",
            "options": [
                "09:00",
                "09:20",
                "09:40",
                "10:00",
                "10:20",
                "10:40",
                "11:00",
                "11:20",
                "11:40",
                "12:00",
                "12:20",
                "12:40",
                "13:20",
                "14:00",
                "14:40",
                "15:20",
                "16:00",
                "16:40",
                "17:20",
                "18:00",
                "18:40",
                "19:20",
                "20:00"
            ],
            "default": "09:00",
            "style": {
                "09:00": null,
                "09:20": null,
                "09:40": null,
                "10:00": null,
                "10:20": null,
                "10:40": null,
                "11:00": null,
                "11:20": null,
                "11:40": null,
                "12:00": null,
                "12:20": null,
                "12:40": null,
                "13:20": null,
                "14:00": null,
                "14:40": null,
                "15:20": null,
                "16:00": null,
                "16:40": null,
                "17:20": null,
                "18:00": null,
                "18:40": null,
                "19:20": null,
                "20:00": null
            },
            "maxLength": 100,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "cRouteses",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "employeeses": {
            "type": "hasMany",
            "foreign": "routes",
            "entity": "CEmployees",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}