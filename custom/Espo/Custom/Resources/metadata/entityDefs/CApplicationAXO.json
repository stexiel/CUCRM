{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "account": {
            "type": "link"
        },
        "attachments": {
            "type": "attachmentMultiple",
            "previewSize": "medium",
            "sourceList": [],
            "accept": [],
            "isCustom": true
        },
        "dateEnd": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "dateStart": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "priority": {
            "type": "enum",
            "options": [
                "Низкий",
                "Обычный",
                "Высокий",
                "Срочно"
            ],
            "style": {
                "Низкий": null,
                "Обычный": "info",
                "Высокий": "warning",
                "Срочно": "danger"
            },
            "default": "Низкий",
            "maxLength": 100,
            "isCustom": true
        },
        "status": {
            "type": "enum",
            "options": [
                "Не началась",
                "Началась",
                "Завершена",
                "Отменена",
                "Отложена"
            ],
            "default": "Не началась",
            "style": {
                "Не началась": null,
                "Началась": "primary",
                "Завершена": "success",
                "Отменена": "info",
                "Отложена": null
            },
            "maxLength": 100,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "cApplicationAXOs",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "attachments": {
            "type": "hasChildren",
            "entity": "Attachment",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true,
            "relationName": "attachments",
            "utility": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}